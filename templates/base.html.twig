<!doctype html><html lang="fr">
<html>
    <head>

        
        <meta charset="utf-8"><meta content="width=device-width, initial-scale=1" name="viewport" />
        {% block stylesheets %}{{ encore_entry_link_tags('app') }}{% endblock %}
        <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/css/bootstrap.min.css" integrity="sha384-Vkoo8x4CGsO3+Hhxv8T/Q5PaXtkKtu6ug5TOeNV6gBiFeWPGFN9MuhOf23Q9Ifjh" crossorigin="anonymous">
        
        <script src="https://code.jquery.com/jquery-3.4.1.slim.min.js" integrity="sha384-J6qa4849blE2+poT4WnyKhv5vZF5SrPo0iEjwBvKU7imGFAV0wwj1yYfoRSJoZ+n" crossorigin="anonymous"></script>
        <script src="https://cdn.jsdelivr.net/npm/popper.js@1.16.0/dist/umd/popper.min.js" integrity="sha384-Q6E9RHvbIyZFJoft+2mJbHaEWldlvI9IOYy5n3zV9zzTtmI3UksdQRVvoxMfooAo" crossorigin="anonymous"></script>
        <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/js/bootstrap.min.js" integrity="sha384-wfSDF2E50Y2D1uUdj0O3uMBJnjuUD4Ih7YwaYd1iqfktj0Uod8GCExl3Og8ifwB6" crossorigin="anonymous"></script>
        
        {% block javascripts %}{{ encore_entry_script_tags('app') }}{% endblock %}
        <link rel="preconnect" href="https://fonts.googleapis.com">
        <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
        <link href="https://fonts.googleapis.com/css2?family=Judson:wght@700&family=Permanent+Marker&family=Poppins:wght@400;700&display=swap" rel="stylesheet">
        
        <link rel="icon" type="image/x-icon" href="{{asset('build/images//favicon-intranet.png')}}">
        {% block head %}
        {% endblock %}
    </head>

    <body>
        {% if is_granted('ROLE_ADMIN') %}
        <nav class="navbar navbar-expand-lg navbar-light bg-blue sticky-nav">
          <a class="navbar-brand" href="/login">Interface administration</a>
          <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNav" aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation">
            <span class="navbar-toggler-icon"></span>
          </button>
          <div class="collapse navbar-collapse" id="navbarNav">
            <ul class="navbar-nav">
              <li class="nav-item ">
                <a class="nav-link" href="/">Accueil </a>
              </li>
              <li class="nav-item">
                <a class="nav-link" href="/adminUser">Editer les utilisateurs</a>
              </li>
              <li class="nav-item">
                <a class="nav-link" href="/edit">Editer les liens</a>
              </li>
              <li class="nav-item">
                <a class="nav-link" href="/modifMessage">Message accueil</a>
              </li>
              <li class="nav-item">
                <a class="nav-link" href="/employee_information">Editer les employés</a>
              </li>
              <li class="nav-item">
                <a class="nav-link" href="/accueilform">Gérer les questionnaires</a>
              </li>
              <li class="nav-item">
                <a class="nav-link" href="/logout">Quitter l'administration</a>
              </li>
            </ul>
          </div>
        </nav>
        {% endif %}
        {% block body %}{% endblock %}
        
        

        {% block js %}{% endblock %}
    </body>
</html>

